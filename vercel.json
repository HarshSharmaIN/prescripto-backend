{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/logo.png",
      "dest": "public/"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
