{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(logo\\.png|favicon\\.ico|.*\\.css|.*\\.js|.*\\.jpg|.*\\.jpeg|.*\\.png|.*\\.svg|.*\\.webp)",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
